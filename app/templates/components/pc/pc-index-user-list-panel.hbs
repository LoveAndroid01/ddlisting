<div class="panel panel-default">
  <div class="panel-heading">排行榜</div>
  <div class="panel-body">
      <ul class="list-group">
          {{#each enabledUserList as |user|}}
            <li class="list-group-item">
                <img src={{user.userProfile}} alt="" class="img-circle" height="30px" />
              {{user.email}}
                <span class="grade">+14</span>
            </li>
          {{/each}}
      </ul>
  </div>
</div>
